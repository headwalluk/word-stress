{
  "name": "word-stress",
  "version": "0.1.0",
  "description": "A command-line tool for stress-testing WordPress and WooCommerce sites",
  "main": "src/main.js",
  "bin": {
    "wordstress": "./bin/wordstress"
  },
  "scripts": {
    "start": "node bin/wordstress",
    "dev": "node bin/wordstress",
    "format": "prettier --write \"src/**/*.js\" \"bin/wordstress\"",
    "format:check": "prettier --check \"src/**/*.js\" \"bin/wordstress\"",
    "lint": "eslint src bin/wordstress",
    "lint:fix": "eslint src bin/wordstress --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "wordpress",
    "woocommerce",
    "stress-testing",
    "load-testing",
    "performance",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=21.0.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^9.39.2",
    "prettier": "^3.7.4"
  }
}
